	</div>

	<h4>Bio: </h4>
	<p><%= @member.bio %></p>

	<%= if Ecto.assoc_loaded?(@member.posts) do %>
		<h4 class="">Submissions:</h4>
		<%= if length(@member.posts) > 0 do %>
			<%= for post <- @member.posts do %>
				<div><a href="/submissions/<%= post.id %>"><%= post.title %></a></div>
			<% end %>
		<% else %>
			<div>None yet!</div>
		<% end %>

	<% end %>


</div>
